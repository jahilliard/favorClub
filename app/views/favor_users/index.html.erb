<p id="notice"><%= notice %></p>

<h1>Listing Favor Users</h1>

<table>
  <thead>
    <tr>
      <th>Favor</th>
      <th>User</th>
      <th>Role</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favor_users.each do |favor_user| %>
      <tr>
        <td><%= favor_user.favor_id %></td>
        <td><%= favor_user.user_id %></td>
        <td><%= favor_user.role %></td>
        <td><%= favor_user.notes %></td>
        <td><%= link_to 'Show', favor_user %></td>
        <td><%= link_to 'Edit', edit_favor_user_path(favor_user) %></td>
        <td><%= link_to 'Destroy', favor_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Favor user', new_favor_user_path %>
